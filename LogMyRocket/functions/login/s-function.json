{
  "name": "login",
  "runtime": "python2.7",
  "description": "Serverless Lambda function for project: LogMyRocket",
  "customName": false,
  "customRole": false,
  "handler": "functions/login/handler.handler",
  "timeout": 6,
  "memorySize": 512,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"],
      "allowCredentials": true
    }
  },
  "endpoints": [
    {
      "path": "login",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiLoginPostTemplate}",
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}